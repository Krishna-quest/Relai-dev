<!-- The Modal -->
<div class="modal permission-modal fade " id="myModal">
  <div class="modal-dialog">
    <div class="modal-content">

      <!-- Modal Header -->
      <div class="modal-header text-center">
        <h2 class="modal-title w-100">Edit Profile</h2>
      </div>

      <!-- Modal body -->
      <div class="modal-body">
        <form class="form" action="/api/update-customer-profile" method="post" id="form3">
          <input type="hidden" name="customer_id" value="<%=session.user_id%>">
          <div class=" row">
            <div class="col-sm-12 col-lg-6">
              <input type="text" readonly class="form-control form-control-fix2" id="fullName" placeholder="Full Name"
                name="cus_fullname" value="<%= session.name%>">
            </div>
            <div class="col-sm-12 col-lg-6">
              <input type=" text" class="form-control form-control-fix2" id="phone Number" placeholder="Phone Number"
                name="cus_phone_number" value="<%= session.phoneNumber%>">
            </div>
          </div>
          <div class=" row">
            <div class="col-sm-12 col-lg-6">
              <input type="text" readonly class="form-control  form-control-fix2" id="address" placeholder="Email Id"
                name="cus_email_id" value="<%= session.email%>">
            </div>
            <div class="col-sm-12 col-lg-6">
              <input type="text" class="form-control  form-control-fix2" id="address" placeholder="Address"
                name="cus_address" value="<%= session.address%>">
            </div>
          </div>

          <div class=" row">
            <div class="col-sm-12 col-lg-6">
              <input type="text" class="form-control  form-control-fix2" id="address" placeholder="City" name="cus_city"
                value="<%= session.city%>">
            </div>
            <div class="col-sm-12 col-lg-6">
              <input type="text" class="form-control  form-control-fix2" id="address" placeholder="Country"
                name="cus_country_id" value="<%= session.country%>">
            </div>
          </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-cancel" data-dismiss="modal">Cancel</button>
        <button name="submit" type="submit" class="btn btn-cancel">Update</button>
      </div>

      </form>
    </div>
  </div>
</div>


<!-- The Modal -->
<div class="modal permission-modal fade " id="changePasswordModal">
  <div class="modal-dialog">
    <div class="modal-content">
      <form class="form" action="#" method="POST" id="changePasswordForm">
      <!-- Modal Header -->
      <div class="modal-header text-center">
        <h2 class="modal-title w-100">Change Password</h2>
      </div>

      <!-- Modal body -->
      <div class="modal-body">
        
          <input type="hidden" name="customer_id" value="<%=session.user_id%>">
          <div class=" col">
            <div class="col-sm-12 col-lg-12">
              <input type="password" class="form-control form-control-fix2" id="oldPassword" placeholder="Old Password"
                name="oldPassword" value="" data-rule-required="true" data-msg-required="Old Password is required!">
            </div>
            <div class="col-sm-12 col-lg-12">
              <input type="password" class="form-control form-control-fix2" id="newPassword" placeholder="New Password"
                name="cus_password" value="" data-rule-required="true" data-msg-required="New Password is required!">
            </div>
            <div class="col-sm-12 col-lg-12">
              <input type="password" class="form-control form-control-fix2" id="confirmPassword"
                placeholder="confirm Password" name="confirmPassword" value="" data-rule-required="true"
                data-msg-required="confirm password">
            </div>
          </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-cancel" data-dismiss="modal">Cancel</button>
        <button name="submit" type="submit" class="btn btn-cancel">Update</button>
      </div>

      </form>
    </div>
  </div>
</div>








<div class="modal permission-modal fade " data-backdrop="static" data-keyboard="false" id="photoModal">
  <div class="modal-dialog">
    <div class="modal-content">

      <!-- Modal Header -->
      <div class="modal-header text-center">
        <h2 class="modal-title w-100">Update profile picture</h2>
      </div>

      <!-- Modal body -->
      <div class="modal-body">
        <form class="form" action="/upload-profile-pic" method="post" id="form3" enctype="multipart/form-data">
          <div class="upload-image">


            <% if( session.imagename=='' ){ %>

              <a href="#" class="upload-profile-pic">
                <img src="images/profile.png" alt="" id="blah" class="profile-photo"> <span>
                  <div class="custom-file-input">
                    <input type="file" id="image" name="profile-pic" value="" onchange="previewFile(this)" required
                      >
                    <input type="button">
                  </div>
                </span>
              </a>
              <% } else {%>
                <a href="#" class="upload-profile-pic">
                  <img src="/upload/<%= session.imagename%>" alt="" id="blah" class="profile-photo"> <span>
                    <div class="custom-file-input">
                      <input type="file" id="image" name="profile-pic" value="" required
                        onchange="previewFile(this)">
                      <input type="button">
                    </div>
                  </span>
                </a>
                <% }%>
          </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-cancel photo-cancel-modal" data-dismiss="modal">Cancel</button>
        <button name="submit" type="submit" class="btn btn-cancel">Upload</button>
      </div>

      </form>
    </div>
  </div>
</div>



<!-- The Modal -->
<div class="modal permission-modal fade " id="MyComplaintModal">
  <div class="modal-dialog">
    <div class="modal-content">
      <form name="complaintform" id="mycomplent_form" method="POST" action="" enctype="multipart/form-data">
        <!-- Modal Header -->
        <div class="modal-header text-center">
          <h2 class="modal-title w-100">Raise a Complaint</h2>
        </div>

        <!-- Modal body -->
        <div class="modal-body">

          <span class="complaint_msg"></span>
          <select class="form-control selectBox " style="height:55px;" id="coms_complaint_help"
            name="coms_complaint_help" data-rule-required="true" data-msg-required="I need help with is required!">
            <option value="">I need help with*</option>
            <option value="Report a bug">Report a bug</option>
            <option value="Property Related Enquiry">Property Related Enquiry</option>
          </select>

          <select data-rule-required="true" data-msg-required="Property is required!" class="form-control selectBox"
            style="height:55px;display:none" id="property_id" name="property_id">
            <option value="">Select Property</option>
          </select>

          <select data-rule-required="true" data-msg-required="Professional is required!" class="form-control selectBox"
            style="height:55px;display:none" id="coms_complaint_for" name="coms_complaint_for">
            <option value="">Select Professional</option>
          </select>

          <input type="hidden" name="cust_user_id" id="cust_user_id" value="<%= session.user_id %>">
          <input type="text" class="form-control  form-control-fix2" id="coms_complaint_subject" placeholder="Subject"
            name="coms_complaint_subject" data-rule-required="true" data-msg-required="Subject is required!">
          <textarea class="form-control  form-control-fix2" rows="3" name="coms_complaint_note" id="coms_complaint_note"
            placeholder="Tell Us More"></textarea>
          <a href="#"  class="upload-any float-right"><i class="fa fa-paperclip float-right" style="color:black"><span
                class="clip-icon">Attach
                files</span></i>
            <div class="custom-file-input">
              <input onchange="previewFile1(this);" type="file" id="image" name="complaint_file" value="">
              <input type="button">
            </div>
            <div class="comp_uploaded_img">
              <img id="uploaded_img" /><br/>
              <span class="imgcompaint_name"></span>
            </div>
          </a>
        </div>

        <div class="modal-footer" >
          <button type="button" class="btn btn-cancel" data-dismiss="modal">Cancel</button>
          <button type="submit" class="btn btn-cancel complain_submit_btn" name="submit">Submit</button>
        </div>
      </form>
    </div>
  </div>

</div>

<!-- Modal -->
<div class="modal fade feedback-modal" id="feedbackModal" tabindex="-1" role="dialog"
  aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
        <h5 class="modal-title">Send Your Feedback</h5>
        <form action="#" method="POST" id="feedbackForm" >
          <div class="form-group">
            <label>Select Rating</label>

            <div class="clearfix">
              <fieldset class="rating">
                <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star5" name="rating" value="5" /><label class="full" for="star5"
                  title="Awesome - 5 stars"></label>
                <!-- <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star4half" name="rating" value="4 and a half" /><label class="half"
                  for="star4half" title="Pretty good - 4.5 stars"></label> -->
                <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star4" name="rating" value="4" /><label class="full" for="star4"
                  title="Pretty good - 4 stars"></label>
                <!-- <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star3half" name="rating" value="3 and a half" /><label class="half"
                  for="star3half" title="Meh - 3.5 stars"></label> -->
                <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star3" name="rating" value="3" /><label class="full" for="star3"
                  title="Meh - 3 stars"></label>
                <!-- <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star2half" name="rating" value="2 and a half" /><label class="half"
                  for="star2half" title="Kinda bad - 2.5 stars"></label> -->
                <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star2" name="rating" value="2" /><label class="full" for="star2"
                  title="Kinda bad - 2 stars"></label>
                <!-- <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star1half" name="rating" value="1 and a half" /><label class="half"
                  for="star1half" title="Meh - 1.5 stars"></label> -->
                <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="star1" name="rating" value="1" /><label class="full" for="star1"
                  title="Sucks big time - 1 star"></label>
                <!-- <input data-rule-required="true" data-msg-required="Message is required!" type="radio" id="starhalf" name="rating" value="half" /><label class="half" for="starhalf"
                  title="Sucks big time - 0.5 stars"></label> -->
              </fieldset>
            </div>

          </div>
          <div class="form-group">
            <label>Enter Your Message</label>
            <textarea  data-rule-required="true" data-msg-required="Message is required!" name="feedback_message" class="form-control" rows="3" placeholder="Message Content"></textarea>
          </div>
          <div>
            <button type="submit" class="btn btn-add-property">Submit</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>