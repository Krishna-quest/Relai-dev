  $("#addTaskForm").on('submit', function (e) {
          e.preventDefault();
          var formData = $(this).serialize();
          if (formData) {
            $.ajax({
              type: "POST",
              data: formData,
              url: "/api/addTask",
              cache: false,
              success: function (data) {
                console.log('data:',data)
                if (data.status == false) {
                  Swal.fire({
                    icon: 'error',
                    title: data.err_msg,
                    showConfirmButton: true,
                    confirmButtonColor: '#005377'
                    //timer: 1500
