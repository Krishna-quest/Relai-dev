<%- include('dashboard_header')-%>
  <%- include('edit-profile-model')-%>
    <div class="dashboard-wpr">
      <div class="container-fluid">
        <div class="row content">
          <div class="col-sm-2 sidenav hidden-xs-down">
            <div class="user-login text-center">

              <%- include('profile-image')-%>

                <div class="user-name">
                  <h2>
                    <%= session.name %>
                  </h2>
                  <a href="#">
                    <%= session.email %>
                      <%- include('edit-profile-icon')-%>
                  </a>
                </div>
            </div>
            <%- include('sidebar')-%>
          </div>
          <div class="col-sm-12 col-lg-10">
            <div class="row complaints-sec">
              <div class="col-sm-12 col-lg-6">
                <div class="complaints">
                  <h2>To do list</h2>
                </div>
              </div>


            </div>
            <div class="todos-inner">

              <div class="todo-list-table">


                <table class="table table-responsive">
                  <thead>
                    <tr>
                      <th>To-dos</th>
                      <th>Status</th>
                      <th>Date</th>
                      <th></th>
                    </tr>
                  </thead>
                  <tbody>
                    <%if(TaskDetailObj.length==0){%>
                      <tr>
                        <td>Result Not Found </td>
                      </tr>
                      <%}else{%>
                        <%TaskDetailObj.ps_phase_array.forEach(function(row,i){%>
                        <tr>
                          <td> <label class="check-todos">
                              <p><%=row.phase_name%></p>
                              <input type="checkbox">
                              <span class="checkmark"></span>
                            </label></td>
                          <td>
                            <%if(row.phase_status=='pending'){%>
                            <a class="btn btn-pending">Pending</a>
                            <%}else{%>
                              <a class="btn btn-complete">Complete</a>
                              <%}%>
                          </td>
                          <td><span><%=row.end_date%></span></td>
                          <td><a class="take-action" href="/mydreamhome-details-phase-a" id="">Take Action</a></td>
                          <!-- <td><a class="take-action" href="javascript:void(0)" id="<%=TaskDetailObj._id%>">Take Action</a></td> -->
                          <!-- <td>
                            <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#todoNotesModal<%=row._id%>"><i class="fa fa-car notesIcon" aria-hidden="true" id="<%=row._id%>"></i></button>
                          </td> -->
                          <!-- <td>
                            <div id="todoNotesModal<%=row._id%>" class="modal fade" role="dialog">
                              <div class="modal-dialog">
                                <div class="modal-content">
                                  <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                                    
                                  </div>
                                  <div class="modal-body">
                                    <p>===========</p>
                                  </div>
                                  <div class="modal-footer">
                                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                  </div>
                                </div>
                            
                              </div>
                            </div>
                          </td> -->
                        </tr>
                        <%});%>
                        <%}%>
                
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    </div>
