    <script>

      $('#refresh').click(function (e) {

        $.ajax({
          type: "POST",
          url: "/api/refresh",
          cache: false,
          success: function (data) {
            if (data.status == false) {
              Swal.fire({
                icon: 'error',
                title: data.message,
                showConfirmButton: false,
                confirmButtonColor: '#005377',
                timer: 4000
              })

            } else {
              Swal.fire({
                icon: 'success',
                title: data.message,
                showConfirmButton: false,
                confirmButtonColor: '#005377',
                timer: 4000
              })
              alert("refresh btn call", data);
              setTimeout(function () {
                //location.reload(true);
                window.location.href = "/mydreamhome";
              }, 4000);

            }
          }
        });
      });

    </script>
