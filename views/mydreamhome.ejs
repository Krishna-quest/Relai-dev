<%- include('dashboard_header')-%>
  <%- include('edit-profile-model')-%>
    <div class="dashboard-wpr">
      <div class="container-fluid">
        <div class="row content">
          <div class="col-sm-2 sidenav hidden-xs-down">
            <div class="user-login text-center">

              <%- include('profile-image')-%>

                <div class="user-name">
                  <h2>
                    <%= session.name %>
                  </h2>
                  <a href="#">
                    <%= session.email %>
                      <%- include('edit-profile-icon')-%>
                  </a>
                </div>
            </div>
            <%- include('sidebar')-%>
          </div>
          <div class="col-sm-12 col-lg-10">
            <div class="dashboard-inner">
              <div class="docs-files-section">
                <div class="row">
                  <div class="col-sm-12 col-lg-12">
                    <div class="docs-files">
                      <div class="clearfix">
                        <div class="heading-top float-left">
                          <h2 class="">My Dream Home</h2>


                        </div>
                        <div class="float-right">
                          <a href="javascript:void(0)" id="refresh">ref</a>
                          <a href="/add-property" class="btn btn-add-property">Add Property</a>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="row">

                  <div class="col-sm-12 col-lg-12">
                    <div class="row">

                      <% var i=0; propertyData.forEach(function(row){ %>
                        <div class=" col-sm-12 col-md-4">

                          <div class="home-section">


                            <div class="my-dream-home-box">

                              <div class="dream-home-img">

                                <%if(propertyImage.length>0){%>

                                  <img src="propimg/<%=propertyImage[i][0].pps_property_image_name%>">
                                  <a href="/add-property?property_id=<%=row._id%>" class="editProperty"
                                    id="<%=row._id%>"><i class="fa fa-edit dream-home-edit-icon "></i></a>
                                  <%}else{%>
                                    <img src="images/nacary.png">
                                    <a href="javascript:void(0)" class=""> <i
                                        class="fa fa-edit dream-home-edit-icon "></i></a>
                                    <%}%>
                                      <% if(row.ps_property_type=='New' ){ %>
                                        <a href="/mydreamhome-details?id=<%=row._id%>"
                                          class="btn btn-new mydreamHomeDetail" value="<%=row._id%>">
                                          <%=row.ps_property_type%>
                                        </a>
                                        <%} else {%>
                                          <a href="/mydreamhome-details-chainproperty?id=<%=row._id%>"
                                            class="btn btn-new mydreamHomeDetail" value="<%=row._id%>">
                                            <%=row.ps_property_type%>
                                          </a>
                                          <%}%>

                              </div>
                              <div class="dream-home-description">
                                <h3><a href="/mydreamhome-details?id=<%=row._id%>">
                                    <%= row.ps_property_name%>
                                  </a></h3>
                                <div class="location-part"><a href="#"><img src="images/location.png">
                                    <%=row.ps_property_address%>,<%=row.ps_property_country_id%>
                                  </a></div>
                                <div class="location-part"><a href="/mydreamhome-details?id=<%=row._id%>"><img
                                      src="images/sq.ft.png">
                                    <%=row.ps_property_area%> sq.ft.
                                  </a></div>
                              </div>
                            </div>
                          </div>
                        </div>
                        <% i++;});%>

                    </div>

                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Optional JavaScript -->
