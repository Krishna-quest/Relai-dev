<script>
  $(document).ready(function () {
    $("#submit_proposal_form").on('submit', function (e) {
      e.preventDefault()
      //var formData = $("#submit_proposal_form").serialize();
      var formData = new FormData($('#submit_proposal_form')[0]);
      console.log(formData)
      //return;
     // alert(formData);
      $('.loader-box').attr('style', 'display:flex !important');
      if (formData) {
        $.ajax({
          type: "POST",
          data: formData,
          url: "/submit-proposal",
          cache: false,
          contentType: false,
          processData: false,
          success: function (data) {
            if (data.status == false) {
              $('.loader-box').attr('style', 'display:none !important');
              Swal.fire({
                // position: 'top-end',
                icon: 'error',
                title: data.message,
                showConfirmButton: true,
                confirmButtonColor: '#005377'
                //timer: 1500
              })
            } else {
              $('.loader-box').attr('style', 'display:none !important');
              console.log('meserrmessagesage:', data.message)


              Swal.fire({
                // position: 'top-end',
                icon: 'success',
                title: data.message,
                showConfirmButton: false,
                confirmButtonColor: '#005377',
                timer: 3000
              })
              location.reload(true)
              $('#notes,#duedate').val('')
            }

          }
        });

      } else {
        alert('some thing wrong')
      }
    });
  })
</script>
