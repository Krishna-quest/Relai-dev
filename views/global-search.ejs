<%- include('dashboard_header')-%>
  <%- include('edit-profile-model')-%>
    <!-- The Modal -->
    <div class="modal fade permission-modal" id="myModal">
      <div class="modal-dialog">
        <div class="modal-content">

          <!-- Modal Header -->
          <div class="modal-header">
            <h2 class="modal-title text-center">Change Permissions</h2>
          </div>

          <!-- Modal body -->
          <div class="modal-body">
            <label class="check-permission"><img src="images/recent-user-1.png">Alex Jackson, <span>Architect</span>
              <input type="checkbox" checked="checked">
              <span class="checkmark"></span>
            </label>
            <label class="check-permission"><img src="images/recent-user-1.png">Michael Williams, <span>Financial
                Advisor</span>
              <input type="checkbox">
              <span class="checkmark"></span>
            </label>
            <label class="check-permission"><img src="images/recent-user-1.png">Rachel Green, <span>Interior
                Designer</span>
              <input type="checkbox">
              <span class="checkmark"></span>
            </label>
            <label class="check-permission"><img src="images/recent-user-1.png">Alex Jackson, <span>Architect</span>
              <input type="checkbox">
              <span class="checkmark"></span>
            </label>
            <label class="check-permission"><img src="images/recent-user-1.png">Michael Williams, <span>Financial
                Advisor</span>
              <input type="checkbox">
              <span class="checkmark"></span>
            </label>
            <label class="check-permission"><img src="images/recent-user-1.png">Rachel Green, <span>Interior
                Designer</span>
              <input type="checkbox">
              <span class="checkmark"></span>
            </label>
          </div>
          <!-- Modal footer -->
          <div class="modal-footer">
            <button type="button" class="btn btn-cancel" data-dismiss="modal">Cancel</button>
            <button type="button" class="btn btn-cancel" data-dismiss="modal">Save</button>
          </div>

        </div>
      </div>
    </div>
    <div class="dashboard-wpr">
      <div class="container-fluid">
        <div class="row content">
          <div class="col-sm-2 sidenav hidden-xs-down">
            <div class="user-login text-center">

              <%- include('profile-image')-%>

                <div class="user-name">
                  <h2>
                    <%= session.name %>
                  </h2>
                  <a href="#">
                    <%= session.email %>
                      <%- include('edit-profile-icon')-%>
                  </a>
                </div>
            </div>
            <%- include('sidebar')-%>
          </div>
          <div class="col-sm-12 col-lg-10">
            <div class="dashboard-inner">
              <div class="docs-files-section">
                <div class="row">
                  <div class="col-sm-12 col-lg-12">
                    <div class="docs-files">
                      <div class="clearfix">
                        <div class="heading-top float-left">
                          <h2 class="">Search Results</h2><br/>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-sm-12 col-lg-12">
                    <div class="row">
                      <% var i=0; if(propertyData.length>0){ propertyData.forEach(function(row){ %>
                        <div class=" col-sm-12 col-md-4">

                          <div class="home-section">


                            <div class="my-dream-home-box">

                              <div class="dream-home-img">

                                <%if(propertyImage.length>0){%>
                                  
                                <%if(propertyImage[i].length>0){%>
                                  <img src="propimg/<%=propertyImage[i][0].pps_property_image_name%>">
                                <%}else{%>
                                      <img src="images/nacary.png">
                                <%}%>

                                <%}%>
                                      <% if(row.ps_property_type=='New' ){ %>
                                        <a href="/mydreamhome-details?id=<%=row._id%>"
                                          class="btn btn-new mydreamHomeDetail" value="<%=row._id%>">
                                          <%=row.ps_property_type%>
                                        </a>
                                        <%} else {%>
                                          <a href="/mydreamhome-details-chainproperty?id=<%=row._id%>"
                                            class="btn btn-new mydreamHomeDetail" value="<%=row._id%>">
                                            <%=row.ps_property_type%>
                                          </a>
                                          <%}%>

                              </div>
                              <div class="dream-home-description">
                                <h3><a href="/mydreamhome-details?id=<%=row._id%>">
                                    <%= row.ps_property_name%>
                                  </a></h3>
                                <div class="location-part"><a href="#"><img src="images/location.png">
                                    <%=row.ps_property_address%>,<%=row.ps_property_country_id%>
                                  </a></div>
                                <div class="location-part"><a href="/mydreamhome-details?id=<%=row._id%>"><img
                                      src="images/sq.ft.png">
                                    <%=row.ps_property_area%> sq.ft.
                                  </a></div>
                              </div>
                            </div>
                          </div>
                        </div>
                        <% i++;});%>
                        <% }else{ %> 
                          <p style="margin-top: 25px;text-align: center; width: 100%;">No data found !! </p>
                        <%}%>

                    </div>

                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    </div>
    </div>
    </body>

    <%- include('dashboard_footer')-%>
   