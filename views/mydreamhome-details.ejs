    <script>

      $(document).ready(function () {
        $('.deleteProfesshional').click(function (e) {
          e.preventDefault();
          var professhional_id = $(this).attr('id');
          //alert(professhional_id)
          var $tr = $(this).closest('tr');
          $.ajax({
            type: "POST",
            url: "/api/removeProfesshional",
            data: {
              professhional_id: professhional_id
            },
            cache: false,
            success: function (data) {
              if (data.status == false) {
                $('.message').html('<span class="alert alert-danger error_validate flash_msg" style="color: red; margin-bottom: 10px;">' + data.err_msg + '</span>');
                setTimeout(function () { $('.message').html(''); }, 3000);
              } else {
                console.log($(".propf_row_" + data.id));
                //$(".propf_row_"+data.id).css("display", "none");
                //$('#removeProfessionalModal').modal('hide');
                $('.message').html('<span class="alert alert-success error_validate flash_msg" style="color: green; margin-bottom: 10px;">' + data.success_msg + '</span>');
                setTimeout(function () { $('.message').html(''); window.location.reload(true) }, 3000);
                $tr.find('td').fadeOut(1000, function () {
                  $tr.remove();
                });
              }
            }
          });
        });

      })
